{{!-- <div class="big-container"> --}}
  <div class="row">
    <div class="col s12 m4 l4">
  <div class="parent-login-form">
    {{#simple-form class="form-field" onsubmit=(action loginParent) as |formValues|}}
      <h2>Parent Login</h2>
      <p class="form-inputs">
        <label class="input-label">Email:</label>
        {{input class="input-field" type="text" placeholder="Email" value=formValues.email}}
      </p>

      <p class="form-field">
        <label class="input-label">Password:</label>
        {{input class="input-field" type="password" placeholder="Password" value=formValues.password}}
      </p>



      <button class="waves-effect waves-light btn">Log In</button>

      {{#link-to "index"}}
      <button class="waves-effect waves-light btn">Cancel</button>
      {{/link-to}}

      {{#link-to "sign-up"}}SignUp{{/link-to}}
    {{/simple-form}}
  </div>
</div>

<div class="col s12 m8 l8">
<div class="parent-pic">
  <img src="kclogo1.png" alt="">
</div>
</div>
</div>
{{!-- </div> --}}
