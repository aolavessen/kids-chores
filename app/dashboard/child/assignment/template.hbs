<h5>Hello {{model.username}}, this is what you have to do</h5>

<ul>
  {{#each model.assignments as |a|}}
    <li>
      {{a.chore.name}}
      <li class="assignment-item">{{assignment.name}}</li>
      {{a.chore.description}}
      <li class="assignment-item">{{assignment.description}}</li>
      <td>
        <button class="waves-effect waves-light btn" {{action isComplete a preventDefault=false}}>Complete</button>
      </td>

    </li>
  {{/each}}
</ul>
