<h1>Add Children</h1>

{{#simple-form onsubmit=(action saveChild) as |formValues|}}
  {{input placeholder="userName" value=formValues.userName}}
  {{input placeholder="password" value=formValues.password}}
  {{input placeholder="avatar" value=formValues.avatar}}
  {{input placeholder="pointTotal" value=formValues.pointTotal}}
  <p>
    {{#link-to "dashboard.parent.children"}}Cancel{{/link-to}}
    <button>SAVE</button>
  </p>

{{/simple-form}}
