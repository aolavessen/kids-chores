<h1>Add Children</h1>

{{#simple-form onsubmit=(action addChild model) as |formValues|}}
  {{input placeholder="Username" value=formValues.username}}
  {{input placeholder="Password" value=formValues.password}}
  <p>
    {{#link-to "dashboard.parent.children.index"}}Cancel{{/link-to}}
    <button>SAVE</button>
  </p>

{{/simple-form}}
